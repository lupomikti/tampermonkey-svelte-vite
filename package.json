{
  "name": "tampermonkey-svelte-vite",
  "version": "1.0.0",
  "description": "Tampermonkey template that uses svelte 5 and Vite to build UserScripts",
  "author": "Lucas Shanley",
  "homepage": "https://github.com/lupomikti/tampermonkey-svelte-vite#readme",
  "type": "module",
  "scripts": {
    "build": "vite build -c ./vite.config.ts",
    "dev": "vite build -c ./vite.config.ts -m development -w",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@tsconfig/svelte": "^5.0.2",
    "@types/tampermonkey": "^5.0.2",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-userscript-metaheader": "^1.0.0",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-preprocess": "^5.1.3",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  }
}
